		<CatDescription>Charts</CatDescription>
		<Type>PalleteEntry</Type>
		<Version>1.0</Version>
	</Header>
	<Metadata>
		<meta name="cdwSupport">true</meta>
	</Metadata>
	<Contents>
		<Model>
			<Property name="width" type="number">width</Property>
			<Property name="height" type="number">height</Property>
